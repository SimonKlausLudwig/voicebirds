{
  "private": true,
  "name": "gatsby-starter-skeleton",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && rm -rf docs && gatsby build && mv public docs && echo \"voicebirds.de\" > docs/CNAME",
    "dev": "gatsby develop",
    "install-collection": "node install-collection.js",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lint:js": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:style": "stylelint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:prettier": "prettier 'src/**/*.{json,md}' --list-different",
    "fix": "yarn fix:js && yarn fix:prettier",
    "fix:js": "yarn lint:js --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "type-check": "tsc"
  },
  "dependencies": {
    "@bit/ludwig.bakers.app": "0.0.1",
    "@bit/ludwig.bakers.arrow-right": "^1.0.1",
    "@bit/ludwig.bakers.body": "^1.0.4",
    "@bit/ludwig.bakers.checkbox": "^1.0.4",
    "@bit/ludwig.bakers.checkbox_alternative": "^1.0.4",
    "@bit/ludwig.bakers.component.index": "^1.0.5",
    "@bit/ludwig.bakers.container": "^1.0.4",
    "@bit/ludwig.bakers.editor": "^1.0.8",
    "@bit/ludwig.bakers.finish.index": "^1.0.7",
    "@bit/ludwig.bakers.grid": "^1.0.1",
    "@bit/ludwig.bakers.index": "^1.0.5",
    "@bit/ludwig.bakers.note": "^1.0.4",
    "@bit/ludwig.bakers.progress": "^1.0.4",
    "@bit/ludwig.bakers.projectapi": "^1.0.9",
    "@bit/ludwig.bakers.question": "^1.0.4",
    "@bit/ludwig.bakers.richtext.index": "^1.0.9",
    "@bit/ludwig.bakers.richtext.style": "^1.0.8",
    "@bit/ludwig.bakers.row": "^1.0.7",
    "@bit/ludwig.bakers.style": "^1.0.6",
    "@bit/ludwig.bakers.textarea": "^1.0.4",
    "@bit/ludwig.bakers.toast": "^1.0.4",
    "@bit/ludwig.bakers.wizardcontrol": "^1.0.6",
    "@msallent/gatsby-theme-skeleton-core": "1.0.2",
    "@msallent/gatsby-theme-skeleton-seo": "1.0.2",
    "babel-plugin-styled-components": "1.10.7",
    "gatsby": "5.9.0",
    "gatsby-image": "2.4.0",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sass": "^2.3.2",
    "gatsby-plugin-scss-typescript": "^4.0.11",
    "gatsby-plugin-sharp": "2.14.4",
    "gatsby-plugin-styled-components": "3.3.0",
    "gatsby-plugin-typescript": "2.4.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-cloudinary": "^0.1.14",
    "gatsby-source-filesystem": "2.3.0",
    "gatsby-transformer-sharp": "2.5.0",
    "gatsby-transformer-typescript-css-modules": "^1.0.14",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-hubspot-form": "^1.3.7",
    "styled-components": "5.1.0"
  },
  "devDependencies": {
    "@types/node": "13.13.6",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "child-process-promise": "^2.2.1",
    "eslint": "7.0.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5",
    "stylelint": "13.3.3",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged",
      "post-commit": "git reset"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "stylelint"
    ],
    "src/**/*.{json,md}": "prettier --write"
  }
}
